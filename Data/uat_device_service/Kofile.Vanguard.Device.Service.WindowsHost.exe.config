<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Microsoft.Practices.Unity.Configuration" />
  </configSections>
  <appSettings configSource="AppSettings.config" />
  
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <!-- ExecutorConfigurations-->
    <alias alias="IDeviceJobConfig" type="Kofile.Vanguard.Device.Infrastructure.IDeviceJobConfig, Kofile.Vanguard.Device.Infrastructure" />
    <alias alias="SignaturePadJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.SignaturePadJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="MagneticCardReaderConfig" type="Kofile.Vanguard.Device.Executors.Entities.MagneticCardReaderConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="FileCopyJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.FileCopyJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="AddressLabelPrinterJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.AddressLabelPrinterJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="ReceiptPrinterJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.ReceiptPrinterJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="PosReceiptPrinterJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.PosReceiptPrinterJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="SlipPrinterJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.SlipPrinterJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="LabelPrinterJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.LabelPrinterJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="GenericPrinterJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.GenericPrinterJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="PrinterJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.PrinterJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="BookPrinterJobConfig" type="Kofile.Vanguard.Device.Executors.Entities.BookPrinterJobConfig, Kofile.Vanguard.Device.Executors.Entities" />
    <!-- Executors-->
    <alias alias="IDeviceJobExecutor" type="Kofile.Vanguard.Device.Infrastructure.IDeviceJobExecutor, Kofile.Vanguard.Device.Infrastructure" />
    <alias alias="SignaturePadJobExecutor" type="Kofile.Vanguard.Device.Executors.SignaturePadJobExecutor, Kofile.Vanguard.Device.Executors" />
    <alias alias="MagneticCardReaderJobExecutor" type="Kofile.Vanguard.Device.Executors.MagneticCardReaderJobExecutor, Kofile.Vanguard.Device.Executors" />
    <alias alias="FileCopyJobExecutor" type="Kofile.Vanguard.Device.Executors.FileCopyJobExecutor, Kofile.Vanguard.Device.Executors" />
    <alias alias="BookPrinterJobExecutor" type="Kofile.Vanguard.Device.Executors.BookPrinterJobExecutor, Kofile.Vanguard.Device.Executors" />
    <!-- ExecuteResultss-->
    <alias alias="IDeviceJobResult" type="Kofile.Vanguard.Device.Infrastructure.IDeviceJobResult, Kofile.Vanguard.Device.Infrastructure" />
    <alias alias="SignaturePadJobResult" type="Kofile.Vanguard.Device.Executors.Entities.SignaturePadJobResult, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="MagneticCardReaderResult" type="Kofile.Vanguard.Device.Executors.Entities.MagneticCardReaderResult, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="FileCopyJobResult" type="Kofile.Vanguard.Device.Executors.Entities.FileCopyJobResult, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="GenericPrinterJobResult" type="Kofile.Vanguard.Device.Executors.Entities.GenericPrinterJobResult, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="LabelPrinterJobResult" type="Kofile.Vanguard.Device.Executors.Entities.LabelPrinterJobResult, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="AddressLabelPrinterJobResult" type="Kofile.Vanguard.Device.Executors.Entities.AddressLabelPrinterJobResult, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="ReceiptPrinterJobResult" type="Kofile.Vanguard.Device.Executors.Entities.ReceiptPrinterJobResult, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="PosReceiptPrinterJobResult" type="Kofile.Vanguard.Device.Executors.Entities.PosReceiptPrinterJobResult, Kofile.Vanguard.Device.Executors.Entities" />
    <alias alias="SlipPrinterJobResult" type="Kofile.Vanguard.Device.Executors.Entities.SlipPrinterJobResult, Kofile.Vanguard.Device.Executors.Entities" />
    <!-- Print Service -->
    <alias alias="IConfigManager" type="Kofile.Vanguard.PrintService.Common.Interfaces.IConfigManager, Kofile.Vanguard.PrintService.Common" />
    <alias alias="IAppSettingsHelper" type="Kofile.Vanguard.Device.Service.Infrastructure.Helpers.Interfaces.IAppSettingsHelper, Kofile.Vanguard.Device.Service" />
    <alias alias="IXmlHelper" type="Kofile.Vanguard.Infrastructure.Serialization.IXmlHelper, Kofile.Vanguard.Infrastructure" />
    <alias alias="IUtility" type="Kofile.Vanguard.Infrastructure.Utility.IUtility, Kofile.Vanguard.Infrastructure" />
   
    <alias alias="ConfigManager" type="Kofile.Vanguard.Device.Service.Infrastructure.ConfigManager, Kofile.Vanguard.Device.Service" />
    <alias alias="AppSettingsHelper" type="Kofile.Vanguard.Device.Service.Infrastructure.Helpers.AppSettingsHelper, Kofile.Vanguard.Device.Service" />
    <alias alias="XmlHelper" type="Kofile.Vanguard.Infrastructure.Serialization.XmlHelper, Kofile.Vanguard.Infrastructure" />
    <alias alias="Utility" type="Kofile.Vanguard.Infrastructure.Utility.Utility, Kofile.Vanguard.Infrastructure" />
    <!-- Printers -->
    <alias alias="IPrinter" type="Kofile.Vanguard.PrintService.Common.Interfaces.IPrinter, Kofile.Vanguard.PrintService.Common" />
    <alias alias="GenericPrinter" type="Kofile.Vanguard.PrintService.GenericPrinter.GenericPrinter, Kofile.Vanguard.PrintService.GenericPrinter" />
    <alias alias="LabelPrinter" type="Kofile.Vanguard.PrintService.LabelPrinter.LabelPrinter, Kofile.Vanguard.PrintService.LabelPrinter" />
    <alias alias="AddressLabelPrinter" type="Kofile.Vanguard.PrintService.LabelPrinter.AddressLabelPrinter, Kofile.Vanguard.PrintService.LabelPrinter" />
    <alias alias="ReceiptPrinter" type="Kofile.Vanguard.PrintService.ReceiptPrinter.ReceiptPrinter, Kofile.Vanguard.PrintService.ReceiptPrinter" />
    <alias alias="PosReceiptPrinter" type="Kofile.Vanguard.PrintService.PosReceiptPrinter.PosReceiptPrinter, Kofile.Vanguard.PrintService.PosReceiptPrinter" />
    <alias alias="SlipPrinter" type="Kofile.Vanguard.PrintService.SlipPrinter.SlipPrinter, Kofile.Vanguard.PrintService.SlipPrinter" />
    <!-- Other -->
    <alias alias="IWebApiConfig" type="Kofile.Vanguard.Common.IWebApiConfig, Kofile.Vanguard.Common" />
    <alias alias="IDeviceService" type="Kofile.Vanguard.Device.Interface.IDeviceService, Kofile.Vanguard.Device.Interface" />
    <alias alias="ITenantConfigService" type="Kofile.Vanguard.Device.Interface.ITenantConfigService, Kofile.Vanguard.Device.Interface" />
    <alias alias="DeviceServiceWebApiConfig" type="Kofile.Vanguard.Device.Service.DeviceServiceWebApiConfig, Kofile.Vanguard.Device.Service" />
    <alias alias="DeviceServiceWebApiClient" type="Kofile.Vanguard.Device.Interface.DeviceServiceWebApiClient, Kofile.Vanguard.Device.Interface" />
    
    <alias alias="IImageExtractor" type="Kofile.Vanguard.Device.Executors.Helpers.IImageExtractor, Kofile.Vanguard.Device.Executors" />
    <alias alias="ZipImageExtractor" type="Kofile.Vanguard.Device.Executors.Helpers.ZipImageExtractor, Kofile.Vanguard.Device.Executors" />

    <alias alias="IOrderProvider" type="Kofile.Vanguard.Device.Executors.Helpers.IOrderProvider, Kofile.Vanguard.Device.Executors" />
    <alias alias="OrderProvider" type="Kofile.Vanguard.Device.Executors.Helpers.OrderProvider, Kofile.Vanguard.Device.Executors" />

    <container>
      <!-- ExecutorConfigurations-->
      <register type="IDeviceJobConfig" mapTo="SignaturePadJobConfig" name="SignaturePadJobConfig" />
      <register type="IDeviceJobConfig" mapTo="MagneticCardReaderConfig" name="MagneticCardReaderConfig" />
      <register type="IDeviceJobConfig" mapTo="FileCopyJobConfig" name="FileCopyJobConfig" />
      <register type="IDeviceJobConfig" mapTo="ReceiptPrinterJobConfig" name="ReceiptPrinterJobConfig" />
      <register type="IDeviceJobConfig" mapTo="PosReceiptPrinterJobConfig" name="PosReceiptPrinterJobConfig" />
      <register type="IDeviceJobConfig" mapTo="SlipPrinterJobConfig" name="SlipPrinterJobConfig" />
      <register type="IDeviceJobConfig" mapTo="GenericPrinterJobConfig" name="GenericPrinterJobConfig" />
      <register type="IDeviceJobConfig" mapTo="LabelPrinterJobConfig" name="LabelPrinterJobConfig" />
      <register type="IDeviceJobConfig" mapTo="AddressLabelPrinterJobConfig" name="AddressLabelPrinterJobConfig" />
      <register type="IDeviceJobConfig" mapTo="BookPrinterJobConfig" name="BookPrinterJobConfig" />
      <!-- Executors-->
      <register type="IDeviceJobExecutor" mapTo="SignaturePadJobExecutor" name="SignaturePadJobConfig" />
      <register type="IDeviceJobExecutor" mapTo="MagneticCardReaderJobExecutor" name="MagneticCardReaderConfig" />
      <register type="IDeviceJobExecutor" mapTo="FileCopyJobExecutor" name="FileCopyJobConfig" />
      <register type="IDeviceJobExecutor" mapTo="GenericPrinter" name="GenericPrinterJobConfig" />
      <register type="IDeviceJobExecutor" mapTo="LabelPrinter" name="LabelPrinterJobConfig" />
      <register type="IDeviceJobExecutor" mapTo="AddressLabelPrinter" name="AddressLabelPrinterJobConfig" />
      <register type="IDeviceJobExecutor" mapTo="ReceiptPrinter" name="ReceiptPrinterJobConfig" />
      <register type="IDeviceJobExecutor" mapTo="PosReceiptPrinter" name="PosReceiptPrinterJobConfig" />
      <register type="IDeviceJobExecutor" mapTo="SlipPrinter" name="SlipPrinterJobConfig" />
      <register type="IDeviceJobExecutor" mapTo="BookPrinterJobExecutor" name="BookPrinterJobConfig" />
      <!-- ExecuteResults-->
      <register type="IDeviceJobResult" mapTo="SignaturePadJobResult" name="SignaturePadJobResult" />
      <register type="IDeviceJobResult" mapTo="MagneticCardReaderResult" name="MagneticCardReaderResult" />
      <register type="IDeviceJobResult" mapTo="FileCopyJobResult" name="FileCopyJobResult" />
      <register type="IDeviceJobResult" mapTo="GenericPrinterJobResult" name="GenericPrinterJobResult" />
      <register type="IDeviceJobResult" mapTo="LabelPrinterJobResult" name="LabelPrinterJobResult" />
      <register type="IDeviceJobResult" mapTo="AddressLabelPrinterJobResult" name="AddressLabelPrinterJobResult" />
      <register type="IDeviceJobResult" mapTo="ReceiptPrinterJobResult" name="ReceiptPrinterJobResult" />
      <register type="IDeviceJobResult" mapTo="PosReceiptPrinterJobResult" name="PosReceiptPrinterJobResult" />
      <register type="IDeviceJobResult" mapTo="SlipPrinterJobResult" name="SlipPrinterJobResult" />
      <!-- Print Service -->
      <register type="IConfigManager" mapTo="ConfigManager" />
      <register type="IAppSettingsHelper" mapTo="AppSettingsHelper" />
      <register type="IXmlHelper" mapTo="XmlHelper" />
      <register type="IUtility" mapTo="Utility" />
      <!-- Other -->
      <register type="IWebApiConfig" mapTo="DeviceServiceWebApiConfig" />
      <register type="IDeviceService" mapTo="DeviceServiceWebApiClient" />
      <register type="ITenantConfigService" mapTo="DeviceServiceWebApiClient" />
      <register type="IImageExtractor" mapTo="ZipImageExtractor" />
      <register type="IOrderProvider" mapTo="OrderProvider" />
      
    </container>
  </unity>
  <system.serviceModel>
    <bindings>
      <netTcpBinding>
        <binding name="NetTcpBinding_ISignaturePadService" />
        <binding name="NetTcpBinding_ICardRearderService" />
      </netTcpBinding>
    </bindings>
    <client>
      <endpoint address="net.tcp://10.25.10.97:8600/SignaturePadService" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ISignaturePadService" contract="SignaturePadService.ISignaturePadService" name="NetTcpBinding_ISignaturePadService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://10.25.10.97:8600/CardReaderService" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ICardRearderService" contract="CardReaderService.ICardRearderService" name="NetTcpBinding_ICardRearderService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
