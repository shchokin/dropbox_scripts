    <switches>
      <add name="Atalasoft.Twain" value="4" />
    </switches>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="DotTwainListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="DotTwain.log" />
      </listeners>
    </trace>
  </system.diagnostics>-->
</configuration>